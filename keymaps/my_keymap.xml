<keymap version="1" name="my_keymap" parent="$default">
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorSelectWord" />
  <action id="EditorUnSelectWord" />
  <action id="FileChooser.TogglePathShowing" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="GotoNextBookmark">
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="GotoPreviousBookmark">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="ctrl o" />
  </action>
  <action id="OverrideMethods" />
  <action id="ParameterInfo" />
  <action id="TabsPlacementNone">
    <keyboard-shortcut first-keystroke="shift alt w" />
  </action>
  <action id="TabsPlacementTop">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="tasks.close" />
</keymap>